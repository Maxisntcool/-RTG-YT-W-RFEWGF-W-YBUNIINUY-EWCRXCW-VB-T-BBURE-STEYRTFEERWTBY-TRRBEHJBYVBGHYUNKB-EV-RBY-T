	{"fling",{"fling"},3,function(player,args)
		local arg1 = string.lower(args[2])

		for i,v in pairs(getPlayer(player,arg1)) do
			local pc = v.Character
			local docont = true
			pc.Humanoid.Died:Connect(function() docont = false end)
